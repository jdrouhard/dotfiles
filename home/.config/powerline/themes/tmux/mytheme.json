{
	"default_module": "powerline.segments.common",
	"segment_data": {
		"system_load": {
			"args": {
				"format": "{avg:.2f}"
			}
		}
	},
	"segments": {
		"right": [
			{
				"name": "uptime",
				"priority": 50
			},
			{
				"name": "external_ip",
				"priority": 50
			},
			{
				"name": "network_load",
				"priority": 50
			},
			{
				"name": "system_load",
				"priority": 50
			},
			{
				"name": "weather",
				"priority": 50,
				"args": {
					"unit": "F",
					"location_query": "66226"
				}
			},
			{
				"name": "date"
			},
			{
				"name": "date",
				"args": {
					"format": "%H:%M",
					"istime": true
				}
			},
			{
				"name": "hostname"
			}
		]
	}
}
